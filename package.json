{
  "name": "@manifoldco/mercury",
  "private": false,
  "main": "lib/index.js",
  "version": "0.0.1",
  "license": "BSD-3",
  "scripts": {
    "build": "diez compile --target web && node scripts/prepare && npm run minify:css",
    "predeploy": "npm run build",
    "deploy": "cd build/diez-manifoldco-mercury-web && npm publish",
    "demo": "diez start web",
    "extract": "diez extract",
    "format": "prettier --write 'src/**/*.ts'",
    "minify:css": "postcss 'build/diez-manifoldco-mercury-web/static/*.css' -d build/diez-manifoldco-mercury-web/static",
    "start": "diez start"
  },
  "dependencies": {
    "@diez/engine": "^10.2.1",
    "@diez/prefabs": "^10.2.0",
    "cssnano": "^4.1.10",
    "postcss-cli": "^7.1.0"
  },
  "devDependencies": {
    "@diez/compiler-core": "^10.2.0",
    "@diez/extractors": "^10.2.0",
    "@diez/start": "^10.2.0",
    "@diez/stdlib": "^10.2.0",
    "@types/eslint-visitor-keys": "^1.0.0",
    "@types/json-schema": "^7.0.4",
    "diez": "^10.2.0",
    "prettier": "^1.19.1",
    "semver": "^7.1.2",
    "typescript": "^3.7.5"
  }
}
